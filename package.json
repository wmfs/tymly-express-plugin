    "body-parser": "1.19.0",
    "@hapi/boom": "9.1.2",
    "cpr": "3.0.1",
    "debug": "4.3.1",
    "dottie": "2.0.2",
    "express": "4.17.1",
    "express-jwt": "5.3.3",
    "helmet": "4.4.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/exec": "5.0.0",
    "@wmfs/tymly": "1.198.0",
